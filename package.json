{
  "name": "ember-aframe-shim",
  "version": "2.6.6",
  "description": "Ember.js shim for A-Frame",
  "keywords": [
    "a-frame",
    "ember-addon"
  ],
  "repository": "https://github.com/ember-vr/ember-aframe-shim.git",
  "license": "MIT",
  "author": "Kelly Selden",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "nodetest": "mocha node-tests --recursive",
    "faltest": "mocha faltest/**/*-test.js --exit"
  },
  "dependencies": {
    "pkg-conf": "^3.0.0",
    "resolve": "^1.8.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@faltest/lifecycle": "^4.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@kellyselden/ember-blueprint": "2.0.0",
    "@percy/webdriverio": "^1.0.4",
    "aframe": "^1.0.0",
    "aframe-event-set-component": "^5.0.0",
    "aframe-layout-component": "^5.3.0",
    "aframe-proxy-event-component": "^2.1.0",
    "babel-eslint": "^10.1.0",
    "ci-info": "^2.0.0",
    "ember-cli": "3.23.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-blueprint-test-helpers": "0.19.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-test-server": "^1.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-extra-blueprint": "0.0.11",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.23.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.15.0",
    "eslint-plugin-ember": "^10.0.2",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "mocha": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "poll-pr-status": "^2.0.0",
    "renovate-config-standard": "^2.0.0"
  },
  "peerDependencies": {
    "aframe": ""
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
